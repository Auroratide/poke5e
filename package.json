{
	"name": "poke5e",
	"version": "1.1.8",
	"scripts": {
		"start": "vite preview",
		"dev": "vite dev",
		"build": "vite build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"clean": "rm -rf build",
		"ci": "npm-run-all clean check build",
		"gh": "gh-pages -d build --dotfiles",
		"publish": "npm-run-all ci gh",
		"script:get-pokemon-images": "node ./scripts/get-pokemon-images.js",
		"script:create-pokemon": "node ./scripts/create-pokemon.js",
		"db:local:start": "docker compose up -d",
		"db:local:stop": "docker compose down",
		"migrate:test:local": "node -r dotenv/config db/test/test.js",
		"migrate:local": "PGPASSWORD=postgres psql -h localhost -f ./db/init-migrations.sql postgres postgres && node -r dotenv/config db/run-migrate.js",
		"migrate:ci": "node db/run-migrate.js",
		"test": "node test/index.test.js",
		"lint": "eslint src --ignore-path .gitignore"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/pg": "^8.11.11",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"dotenv": "^16.4.7",
		"eslint": "8.57.0",
		"eslint-plugin-only-error": "^1.0.2",
		"eslint-plugin-svelte": "^2.46.1",
		"gh-pages": "^3.2.3",
		"npm-run-all": "^4.1.5",
		"pg": "^8.13.3",
		"postgres-migrations": "^5.3.0",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"svelte-preprocess": "^5.1.4",
		"taiko": "^1.4.3",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"vite": "^5.4.14"
	},
	"type": "module",
	"license": "ISC",
	"author": {
		"name": "Timothy Foster",
		"url": "https://auroratide.com"
	},
	"dependencies": {
		"@auroratide/pokemon-types": "^0.1.0",
		"@auroratide/toggle-switch": "^0.3.1",
		"@supabase/supabase-js": "^2.48.1"
	},
	"packageManager": "pnpm@8.15.1",
	"engines": {
		"pnpm": "^8.0.0"
	}
}