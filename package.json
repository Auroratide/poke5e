{
  "name": "poke5e",
  "version": "0.0.1",
  "scripts": {
    "start": "vite preview",
    "dev": "vite dev",
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rm -rf build",
    "ci": "npm-run-all clean check build",
    "gh": "gh-pages -d build --dotfiles",
    "publish": "npm-run-all ci gh",
    "script:get-pokemon-images": "node ./scripts/get-pokemon-images.js",
    "migrate": "node -r dotenv/config db/migrate.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.48",
    "@sveltejs/kit": "^1.0.0-next.561",
    "dotenv": "^16.0.3",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "postgres-migrations": "^5.3.0",
    "sass": "^1.56.1",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.1",
    "typescript": "~4.9.3",
    "vite": "^3.2.4"
  },
  "type": "module",
  "license": "ISC",
  "author": {
    "name": "Timothy Foster",
    "url": "https://auroratide.com"
  },
  "dependencies": {
    "@auroratide/pokemon-types": "^0.1.0",
    "@supabase/supabase-js": "^2.1.1"
  },
  "packageManager": "pnpm@7.13.3",
  "engines": {
    "pnpm": "^7.0.0"
  }
}
