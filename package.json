{
  "name": "poke5e",
  "version": "0.0.1",
  "scripts": {
    "start": "vite preview",
    "dev": "vite dev",
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rm -rf build",
    "ci": "npm-run-all clean check build",
    "gh": "gh-pages -d build --dotfiles",
    "publish": "npm-run-all ci gh",
    "script:get-pokemon-images": "node ./scripts/get-pokemon-images.js",
    "migrate": "node -r dotenv/config db/migrate.js",
    "test": "node test/index.test.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.6",
    "@sveltejs/kit": "^1.30.4",
    "dotenv": "^16.4.5",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "postgres-migrations": "^5.3.0",
    "sass": "^1.77.2",
    "svelte": "^3.59.2",
    "svelte-check": "^3.7.1",
    "svelte-preprocess": "^5.1.4",
    "taiko": "^1.3.10",
    "tslib": "^2.6.2",
    "typescript": "~4.9.5",
    "vite": "^4.5.3"
  },
  "type": "module",
  "license": "ISC",
  "author": {
    "name": "Timothy Foster",
    "url": "https://auroratide.com"
  },
  "dependencies": {
    "@auroratide/pokemon-types": "^0.1.0",
    "@supabase/supabase-js": "^2.43.2"
  },
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "pnpm": "^8.0.0"
  }
}
